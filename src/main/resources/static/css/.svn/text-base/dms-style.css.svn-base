/*dms-style.css*/
a {
	text-decoration: none;
}
img {
	border: none;
}
ul , li{
	list-style-type: none; 
	margin: 0;
}
ul {
	padding: 0 0 0 20px;
}
.pushLeft {
	float: left;
}
.pushRight {
	float: right;
}
.clearfix:before,
.clearfix:after{
	content:" ";
	display:table;
	visibility:hidden
}
.clearfix:after{
	clear:both
}
.clearfix{
	clear:both;
}
.wrapper {
	width: 100%;
}
.dashboard {
	margin: 0 auto;
}
.dashboard .panel {
	width: 30%;
    border-radius: 2px;
	background-color: #FFFFFF;
	border: 1px solid #EBEFF6;
	display: inline-block;
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05);
	margin-bottom: 30px;
	margin-left: 2.2%;
	height: 280px;
	overflow: hidden;
}
@media all and (max-width: 400px) {
	.dashboard .panel {
		display: block;
		width: 94%;
	}
}
.dashboard .panel-heading {
	background-color: #015198;;
	color: #DBD8E9;
	border-color: #EBEFF6;
    border-radius: 2px 2px 0 0;
	padding: 10px 15px;
}
.dashboard .panel-content {
	padding: 0;
}
.dashboard .panel-content p {
	padding: 10px 15px;
	margin: 0;
}
.dashboard .panel-content-list-item {
	color: #555555;
    padding-right: 15px;
	background-color: #FFFFFF;
    border-top: 1px solid #EBEFF6;
    display: block;
    margin-bottom: -1px;
    padding: 10px 15px;
    position: relative;
}
.panel-content-list-item  span{
	float: left;
}
.panel-content-list-item  .list-name {
	padding: 0 15px 0 5px;
}
.panel-content-list-item  .list-count {
	float: right;
	background-color: #92CF5C;
    color: #FBFDFA;
    border-radius: 10px;
    display: inline-block;
    font-size: 12px;
    font-weight: bold;
    line-height: 1;
    min-width: 10px;
    padding: 3px 7px;
    text-align: center;
    vertical-align: baseline;
    white-space: nowrap;
}

/*inner page*/
.folder-hierarchy,
.manage-document  {
	margin: 2% 0;
}
.folder-hierarchy .content,
.manage-document .collapseDiv {
    border-radius: 2px;
	background-color: #FFFFFF;
	border: 1px solid #EBEFF6;
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05);
	overflow: hidden;
}
.folder-hierarchy {
  float: left;
  width: 30%; 
  min-height: 500px;
  margin-left: 2%;
}
.manage-document {
  float: right;
  width: 63%;
  margin-left: 2%;
  margin-right: 2%;
}
@media all and (max-width: 400px) {
	.folder-hierarchy,
	.manage-document {
		float: none;
		width: 96%;   
	}
	.folder-hierarchy {
		min-height:100px; 
		margin-bottom: 30px;
	}
}
.manage-document header a {
	float: right;
}
.manage-document .collapseDiv {
	margin-bottom: 30px;
}
.folder-hierarchy .content,
.manage-document .content {
	padding: 10px 15px;
}
.manage-document .content {
	min-height: 200px;
}
.button-container {
	padding-right: 15px;
}
/***************************/
.folder-action input[type="text"],
.file-action input[type="text"] {
    background-color: #FFFFFF;
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: #555555;
    /*display: block;*/
    font-size: 14px;
    height: 34px;
    line-height: 20px;
    padding: 0 12px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
    vertical-align: middle;
    /*width: 100px;*/
	width:auto;
	margin-right:10px;
}
.folder-action .button,
.file-action .button {
    border-radius: 5px;
    cursor: pointer;
    font-size: 1x;
    padding: 2px;
    text-decoration: none;
    background-color: #92CF5C;
    border: 1px solid #92CF5C;
    color: #FFFFFF;
    margin: 5px 5px 5px 0;
    text-transform: uppercase;
    width: auto;
}
.form-action input[type="text"] {
    background-color: #FFFFFF;
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: #555555;
    display: block;
    font-size: 14px;
    height: 34px;
    line-height: 20px;
    padding: 0 12px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
    vertical-align: middle;
    width: 200px;	
	float:left;
	margin-right:10px;
}

.form-action .textarea {
    background-color: #FFFFFF;
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    height: auto;
    padding: 10px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
}
.form-action .select {
    background-color: #FFFFFF;
    border: 1px solid #CCCCCC;
    border-radius: 4px;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset;
    color: #555555;
    display: block;
    font-size: 14px;
    height: 34px;
    line-height: 20px;
    padding: 5px 2px;
    transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
    vertical-align: middle;
    width: 140px;
	margin-right:10px;
}
.button {
    border-radius: 5px;
    cursor: pointer;
    font-size: 12x;
    padding: 6px 6px;
    text-decoration: none;
    background-color: #92CF5C;
    border: 1px solid #92CF5C;
    color: #FFFFFF;
    margin: 10px 10px 10px 0;
    text-transform: uppercase;
    width: auto;
}
.button:hover, .button:focus, .button:active{
    background-color: #7DC63C;
    border-color: #7DC63C;
    transition: all 0.2s linear 0s;
}

.button.blue {
    background-color: #87D641;
    border-color: #87D641;
}
.button.blue:hover {
    background-color: #5DCFF3;
    border-color: #50C1E5;
}

.button.red {
    background-color: #FB6B5B;
    border-color: #FB6B5B;
}
.button.red:hover {
    background-color: #EE6252;
    border-color: #EE6252;
}
/* Starts: Styling for input type file */
span.upload,
span.uploadFile {
	float: left; 
    display: block;
}
span.upload {
	background-color: #92CF5C;
    border: 1px solid #92CF5C;
    border-radius: 5px;
    color: #FFFFFF;
    cursor: pointer;
    height: 18px;
    margin: 10px 10px 10px 0;
    overflow: hidden;
    padding: 6px 12px;
    position: relative;
    text-decoration: none;
    text-transform: uppercase;
    width: 66px;
}

span.upload input {
    cursor: pointer;
    display: block ;
    height: 18px ;
    left: 0;
    margin-top: -27px;
    opacity: 0 ;
    overflow: hidden ;
    padding: 6px 12px;
    position: absolute;
    right: 0;
    width: 66px;
}
.form-action .uploadFile input {
	border: none;
	box-shadow: none;
	margin: 8px 0;
}
.main{
padding:9px!important;
}
.dms-h{
max-height:160px;
height:160px;
}
/* Ends: Styling for input type file */